{
    "name": "forci/menu-builder-bundle",
    "type": "symfony-bundle",
    "description": "A simple, standalone Menu Builder for Symfony 2 Applications",
    "keywords": ["wucdbm", "menu", "builder", "menu builder", "symfony2"],
    "authors": [
        {
            "name": "Martin Kirilov",
            "email": "wucdbm@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "wucdbm/quick-ui-bundle": "~0.2",
        "symfony/symfony": "~3.0|~4.0"
    },
    "require-dev": {
        "doctrine/orm": "~2.5",
        "friendsofphp/php-cs-fixer": "^2.8",
        "wucdbm/php-cs-fixers": "~0.2"
    },
    "autoload": {
        "psr-4": { "Forci\\Bundle\\MenuBuilderBundle\\": "src" }
    },
    "config": {
